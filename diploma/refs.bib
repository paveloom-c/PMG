@article{S.I.1978,
  title = {The {{Space Distribution}} of {{Globular Clusters}}},
  author = {Sasaki, T. and Ishizawa, T.},
  date = {1978-10-01},
  journaltitle = {Astronomy and Astrophysics},
  volume = {69},
  pages = {381},
  issn = {0004-6361},
  url = {https://ui.adsabs.harvard.edu/abs/1978A&A....69..381S},
  urldate = {2023-05-09},
  abstract = {Summary. The space distribution of globular clusters in our Galaxy is divided into three regions; a cone of avoidance around the rotation axis (COA), a faint cluster region, and a bright cluster region. The causes of such a division, especially of the COA, are studied. The orbits of test clusters are calculated over 2 1010 yr in the Schmidt (1965) mass model. Initially 63 test clusters are distributed in a sphere of radius 100 kpc and rotated with the angular velocity of a uniform sphere of radius 100 kpc with the mass and the angular momentum of our Galaxy. The tidal radius and, if possible, the increase in total energy by an impulsive approximation for a typical structure of globular clusters are calculated at each perigalactic passage to estimate the disruptive effect of the tidal field near the galactic center. It is found that clusters initially in the COA reappear there only for 14\% of the total time and, as a result, the density in the COA is considerably rarefied compared to that outside the COA, and that most of them are disrupted by the tidal force of the galactic center. The other clusters cannot enter the COA and clusters with large tidal radius always stay in the bright cluster region. The three division of galactic space is therefore interpreted by the instability of an orbit with low angular momentum and the tidal disruptive effect. The apparent frequency distribution of angular momenta of surviving test clusters derived on the assumption that they have no tangential motion with respect to the Sun shows a peak around zero, though their real distribution does not show such a peak. We make a new estimate of the distance to the galactic center Ro=9.2+l.3 (m.e.) kpc in such a way as to maximize the size of the COA. Key words: globular clusters - cone of avoidance - distance to the galactic center},
  langid = {english},
  annotation = {ADS Bibcode: 1978A\&A....69..381S},
  file = {/home/paveloom/Zotero/storage/M2H54Q4I/Sasaki and Ishizawa - 1978 - The Space Distribution of Globular Clusters.pdf}
}

