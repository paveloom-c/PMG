  langid = {english},
  keywords = {Astrophysics - Astrophysics of Galaxies,Galaxy (Milky Way),kinematics,masers,rotation curve,solar Galactocentric distance},
  annotation = {ADS Bibcode: 2014AstL...40..389B},
  file = {/home/paveloom/Zotero/storage/XYT3NYP3/Bobylev and Bajkova - 2014 - Determination of galactic rotation parameters and .pdf}
